WHITESPACE = _{ " " | "\t" }
COMMENT = _{ "#" ~ commentchar* ~ NEWLINE* }
commentchar = _{ !NEWLINE ~ ANY }
wordchar = _{ !(NEWLINE | WHITESPACE | "#" | "=" | "{" | "}") ~ ANY }
key = { wordchar+ }
value = { (wordchar+) | list }
pair = { key ~ "=" ~ value }
list = { "{" ~ NEWLINE* ~ (pair ~ NEWLINE*)+ ~ NEWLINE* ~ "}" }
file = { SOI ~ (NEWLINE* ~ pair ~ NEWLINE*)+ ~ EOI }
